<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="v1-4-fill-passengers-table" author="Nikita Przhevalski">
        <loadData tableName="passengers" file="db/changelog/v1/data/passengers-data.csv"/>
        <rollback>
            <sqlFile dbms="postgresql"
                     path="db/changelog/v1/sql/v1-delete-passenger-data.sql"/>
        </rollback>
    </changeSet>

    <changeSet id="v1-5-fill-cards-table" author="Nikita Przhevalski">
        <loadData tableName="cards" file="db/changelog/v1/data/cards-data.csv"/>
        <rollback>
            <sqlFile dbms="postgresql"
                     path="db/changelog/v1/sql/v1-delete-card-data.sql"/>
        </rollback>
    </changeSet>

    <changeSet id="v1-6-fill-passengers-cards-table" author="Nikita Przhevalski">
        <loadData tableName="passengers_cards" file="db/changelog/v1/data/passengers-cards-data.csv"/>
        <rollback>
            <sqlFile dbms="postgresql"
                     path="db/changelog/v1/sql/v1-delete-passenger-card-data.sql"/>
        </rollback>
    </changeSet>

</databaseChangeLog>