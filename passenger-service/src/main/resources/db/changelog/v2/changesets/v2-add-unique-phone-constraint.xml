<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="v2-1-add-unique-phone-column-constraint-passenger-table" author="Nikita Przhevalski">
        <addUniqueConstraint tableName="passengers" columnNames="phone" constraintName="passenger_phone_unique_c"/>
        <rollback>
            <sqlFile dbms="postgresql"
                     path="db/changelog/v2/sql/v2-drop-phone-unique-constraint.sql"/>
        </rollback>
    </changeSet>

</databaseChangeLog>