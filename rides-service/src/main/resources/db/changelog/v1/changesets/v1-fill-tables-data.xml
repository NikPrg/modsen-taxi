<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="v1-3-fill-drivers-info-table" author="Nikita Przhevalski">
        <loadData tableName="drivers_info" file="db/changelog/v1/data/drivers-info-data.csv"/>
        <rollback>
            <sqlFile dbms="postgresql"
                     path="db/changelog/v1/sql/v1-delete-drivers-info-data.sql"/>
        </rollback>
    </changeSet>

    <changeSet id="v1-4-fill-rides-table" author="Nikita Przhevalski">
        <loadData tableName="rides" file="db/changelog/v1/data/rides-data.csv"/>
        <rollback>
            <sqlFile dbms="postgresql"
                     path="db/changelog/v1/sql/v1-delete-rides-data.sql"/>
        </rollback>
    </changeSet>

</databaseChangeLog>